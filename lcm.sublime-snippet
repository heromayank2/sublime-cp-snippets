<snippet>
	<content><![CDATA[
int gcd(int a,int b){
	// HCF is GCD
	if(b==0)return a;
	return gcd(b,a%b);
}

int lcm(int a,int b){
	return (a*b/gcd(a,b));
}
]]></content>
	<!-- Optional: Set a tabTrigger to define how to trigger the snippet -->
	<tabTrigger>lcm</tabTrigger>
	<!-- Optional: Set a scope to limit where the snippet will trigger -->
	<!-- <scope>source.python</scope> -->
</snippet>
